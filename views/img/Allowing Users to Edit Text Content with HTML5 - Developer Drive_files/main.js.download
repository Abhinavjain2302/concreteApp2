function saveMenuOffset(){
  var active_menu_block = jQuery('#main-menu-wrap .active');
  Set_Cookie('menu_offset', Math.round(active_menu_block.position().left), 0, '/' );
}

function initMenuSlider(){
  //var wrap = jQuery('#main-menu-wrap');
  var wrap = jQuery('#menu-ext'); //jQuery('#main-menu-gap');
  var wrapWidth = wrap.width();
          
  jQuery('.menu-block a').click(function(){
     saveMenuOffset();   
  });
  
  //menuWrapWidth = wrap.width() - 80;//
  menuWrapWidth = jQuery('#main-menu-wrap').width() - 50;//wrap.width();
 
  wrap.mousemove(function(event){
    
    if(activeMenuWidth < menuWrapWidth)
      return;
    
    
    menuMouseX = event.pageX - wrap.offset().left;
    if(menuMouseX < 70)
      menuMouseX = 70;
      
    if(menuMouseX > wrapWidth - 40)
      menuMouseX = wrapWidth - 40;
    
    
    menuMouseX = menuMouseX - 80;//layerX;
   
    
  });
  

  
  wrap.mouseleave(function(){
   
     clearInterval(mouseHandleIntervalId);
     mouseHandleIntervalId = null;
  });
  
  wrap.mouseenter(function(){
     if(initMenuAnimation()){
      mouseHandleIntervalId = setInterval('menuAnimation()', 25);
      
     }
  });
  
  

}

function initMenuAnimation(){
  activeMenuBlock = jQuery('#main-menu-wrap .active');
  currentMenuX = activeMenuBlock.position().left;
  var lastChild = jQuery(activeMenuBlock.find('a').last());
  activeMenuWidth = lastChild.position().left + lastChild.outerWidth();
  if(activeMenuWidth < menuWrapWidth)
    return false;
  return true;
}


function menuAnimation(){
  
  //var x = menuMouseX * (1 - activeMenuWidth / menuWrapWidth);
  var shift = menuWrapWidth - activeMenuWidth;
  var percent = menuMouseX / menuWrapWidth;
  var x = (shift * percent);
  //activeMenuBlock.css('left', x + 'px');
  
  //return
  
  
  var difference = currentMenuX - x;
  
  var newOffsetX = difference * 0.1;
  currentMenuX -= newOffsetX;
  //console.log(currentX);
  activeMenuBlock.css('left', currentMenuX + 'px');
  //console.log(menuMouseX + ' ' + x + ' ' + newOffsetX + ' ' + currentX);
  //console.log(difference);
  //activeMenuBlock.css('left', currentX + 'px');
  
  //var x=this._parent._xmouse*(1-this._parent.thumbs._width/this._width)
	//var difference = this._parent.thumbs._x - x;
	//this._parent.thumbs._x -= (difference/this._parent.velocity);

}


function initMenuSwitcher(){
   
   
   jQuery('#menu-switcher a').click(function(){
     var active_link = jQuery('#menu-switcher a.active');
     var prevIndex = active_link.getIndex();
     
     var current_link = jQuery(this);
     var index = current_link.getIndex();
     
     if(index == prevIndex)
      return;
     
     var active_menu_block = jQuery('#main-menu-wrap .active');
     active_menu_block.hide();
     
     active_menu_block.removeClass('active');
     active_link.removeClass('active');
     
     
     
     
     Set_Cookie( 'menu_index', index, 0, '/' );
     
     var menu_block = jQuery(jQuery('#main-menu-wrap .menu-block').get(index));
     menu_block.css('top', '60px');
     menu_block.show();
     current_link.addClass('active');
     menu_block.animate({top: 18}, 200, function(){
      active_menu_block.css( {'top':'60px', 'left' : '0px' });
      
      menu_block.addClass('active');
      saveMenuOffset();
      
     });
     
     
     
     
     
     return false;
   });
}

jQuery.fn.getIndex = function(){
	  var $p=jQuery(this).parent().children();
    return $p.index(this);
}

function getRmId(elem){
  if(!elem.attr('id')){
    var newId = 'rmid_' + idHashIndex;
    elem.attr('id', newId);
    idHashIndex++;
    return newId;
  }
  
  return elem.attr('id');
 
}

function makeUpperStep(readMoreId){
  var data = elementToIdHash[readMoreId];
  var txtElem = jQuery(data.elem.find('.read-more-wrap')[0]);
  var text = txtElem.html();
  var len = text.length;
  data.index++;
  var readMoreIndex = data.index;
  if(readMoreIndex > len - 1 || readMoreIndex < 1){
    clearInterval(data.IntervalId);
    return;
  }
    
  var c = text[readMoreIndex];
  var cCode = c.charCodeAt(0);
  if(c < 97 || c > 122 ){
    makeUpperStep(readMoreId);
    return;
  }
  
  text = text.setCharAt(readMoreIndex, c.toUpperCase());
  txtElem.html(text);
}

function makeLowerStep(readMoreId){
  var data = elementToIdHash[readMoreId];
  var txtElem = jQuery(data.elem.find('.read-more-wrap')[0]);
  var text = txtElem.html();
  var len = text.length;
  data.index--;
  var readMoreIndex = data.index;
  if(readMoreIndex < 1 || readMoreIndex > len - 1 ){
    clearInterval(data.intervalId);
    return;
  }
    
  var c = text[readMoreIndex];
  var cCode = c.charCodeAt(0);
  if( c < 65 || c > 90 ){
    makeLowerStep(readMoreId);
    return;
  }
    
  
  text = text.setCharAt(readMoreIndex, c.toLowerCase());
  txtElem.html(text);
   
}

String.prototype.setCharAt = function(index,chr) {
	if(index > this.length-1) return str;
	return this.substr(0,index) + chr + this.substr(index+1);
}


function Set_Cookie( name, value, expires, path, domain, secure )
{
// set time, it's in milliseconds
var today = new Date();
today.setTime( today.getTime() );


if ( expires )
{
expires = expires * 1000 * 60 * 60 * 24;
}
var expires_date = new Date( today.getTime() + (expires) );

document.cookie = name + "=" +escape( value ) +
( ( expires ) ? ";expires=" + expires_date.toGMTString() : "" ) +
( ( path ) ? ";path=" + path : "" ) +
( ( domain ) ? ";domain=" + domain : "" ) +
( ( secure ) ? ";secure" : "" );
}





function Get_Cookie( check_name ) {
	// first we'll split this cookie up into name/value pairs
	// note: document.cookie only returns name=value, not the other components
	var a_all_cookies = document.cookie.split( ';' );
	var a_temp_cookie = '';
	var cookie_name = '';
	var cookie_value = '';
	var b_cookie_found = false; // set boolean t/f default f

	for ( i = 0; i < a_all_cookies.length; i++ )
	{
		// now we'll split apart each name=value pair
		a_temp_cookie = a_all_cookies[i].split( '=' );


		// and trim left/right whitespace while we're at it
		cookie_name = a_temp_cookie[0].replace(/^\s+|\s+$/g, '');

		// if the extracted name matches passed check_name
		if ( cookie_name == check_name )
		{
			b_cookie_found = true;
			// we need to handle case where cookie has no value but exists (no = sign, that is):
			if ( a_temp_cookie.length > 1 )
			{
				cookie_value = unescape( a_temp_cookie[1].replace(/^\s+|\s+$/g, '') );
			}
			// note that in cases where cookie is initialized but no value, null is returned
			return cookie_value;
			break;
		}
		a_temp_cookie = null;
		cookie_name = '';
	}
	if ( !b_cookie_found )
	{
		return null;
	}
}



