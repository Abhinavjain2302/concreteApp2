function initOipLinks(){
  jQuery('.oip-lnk').click(function(e){
    e.preventDefault();
    var url = jQuery(this).attr('href');
    jQuery('body').addClass('oip-active');
    jQuery.ajax({
      url: url,
      data : {ajax : 1, theme : 'dd2016' },
      success : function(data){
        jQuery('#oip-text').html(data);
        
        var winGap = 100;
        var maxHeight = jQuery(window).height() - winGap;
        if(maxHeight < 600 ){
          jQuery('#oip-content').css('max-height', maxHeight);
        }
        
        var height = jQuery('#oip-content').height();
        
        jQuery('#oip-content').css('top', - (height + winGap) * 0.5);
          
      }
    });
    
    return false;
  });
  
  jQuery('#oip-close').click(function(event){
     event.preventDefault();
     jQuery('body').removeClass('oip-active');
     return false;
  });
  
  jQuery(document).click(function(event) { 
    if(!jQuery(event.target).closest('#oip-content').length &&
       !jQuery(event.target).is('#oip-content')) {
        jQuery('body').removeClass('oip-active');
    }        
  });
}

