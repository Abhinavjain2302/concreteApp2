        alert('Thank you for subscribing to the DeveloperDrive newsletter!')
      }
    });  
    
    
    
    return false;
  });
  
}

function fixFontSizes(){
  jQuery('.hp-post-title a').each(function(i,e){
     var t = jQuery(e);
     var pBlock = t.closest('.hppp-title-block');
     if( t.width() > pBlock.width() ){
       
       var size = parseInt(t.css('font-size').replace('px',''));
       do{
         
         size --;
         t.css('font-size', size + 'px');
         if(size < 12){
           break;
         }
         
       }
       while( t.width() > pBlock.width() );  
     }
  });
  
  
  jQuery('.hppp-auth-lb').each(function(i,e){
     var t = jQuery(e);
     var pBlock = t.closest('.hppp-author');
     
     if( t.width() > pBlock.width() ){
       
       var size = parseInt(t.css('font-size').replace('px',''));
       
       do{
         
         size --;
         t.css('font-size', size + 'px');
         if(size < 12){
           break;
         }
         
       }
       while( t.width() > pBlock.width() );
       
         
     }
  });
}


