var loadingNextEntries = false;
var firstMoreLoaded = false;
function loadNextPosts(){
  if(loadingNextEntries)
    return;
    
  var loadBtn = jQuery('#load-more-btn');  
  var page = parseInt( loadBtn.attr('data-page') );
  page++;
  
     
  var url = loadBtn.attr('data-url');
  if(url){
    loadingNextEntries = true;
    url = url.replace(/\/page\/\d+/i, '/page/' + page);
    loadBtn.html('Loading<span class="ld-one">.</span><span class="ld-two">.</span><span class="ld-three">.</span>â€‹');
  }
  
  
  
     
    jQuery.ajax( { url: url, type: "GET", data: { ajax: 1}, 
      success: function(data){
         loadingNextEntries = false;
         loadBtn.text('Load more');
         jQuery('#posts-list').append(data);
         
         if(!firstMoreLoaded){
           //jQuery('#rect-ads-cont').append(jQuery('#bottom-rect-ads'));
           firstMoreLoaded = true;
         }
         
      } 
    });
    loadBtn.attr('data-page', page);
    loadBtn.attr('href', url);
}


