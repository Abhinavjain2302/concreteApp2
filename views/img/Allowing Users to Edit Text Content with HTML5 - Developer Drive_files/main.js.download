function drawPopularTriangle(context){
  context.beginPath();
  context.moveTo(7, 0);        // Top Corner
  context.lineTo(14,8); // Bottom Right
  context.lineTo(0, 8);         // Bottom Left
  context.closePath();
  
  // Fill the path
  context.fillStyle = "#d9d9d9";
  context.fill();
}

function drawPopSharesTriangle(context){
  context.beginPath();
  
  context.moveTo(0, 0);        // Top Corner
  context.lineTo(7, 7);         // Bottom Left
  context.lineTo(14,0); // Bottom Right
  context.fillStyle = "#fff";
  context.fill();
  context.closePath();
  
  
  context.beginPath();
  
  context.moveTo(0, 0);        // Top Corner
  context.lineTo(7, 7);         // Bottom Left
  context.lineTo(14,0); // Bottom Right 
  context.strokeStyle = '#ddd';
  context.lineWidth   = 1;
  context.stroke();
  
}

function drawPinkRectTriangle(context){
  
  // Draw a path
  context.beginPath();
  context.moveTo(0, 40);        // Top Corner
  context.lineTo(5,0); // Bottom Right
  context.lineTo(15, 0);         // Bottom Left
  context.lineTo(8, 40);         // Bottom Left
  context.closePath();

  // Fill the path
  context.fillStyle = "#f4bcb8";
  context.fill();
}

function drawDarkRedRectTriangle(context){
  
  // Draw a path
  context.beginPath();
  context.moveTo(0, 0);        // Top Corner
  context.lineTo(0,40); // Bottom Right
  context.lineTo(7, 40);         // Bottom Left
  context.lineTo(10, 0);         // Bottom Left
  context.closePath();

  // Fill the path
  context.fillStyle = "#b63932";
  context.fill();
}




function drawLeftTriangle(context){
  var width = 8;  // Triangle Width
  var height = 8; // Triangle Height
  

  // Draw a path
  context.beginPath();
  context.moveTo(8, 0);        // Top Corner
  context.lineTo(8,8); // Bottom Right
  context.lineTo(0, 0);         // Bottom Left
  context.closePath();

  // Fill the path
  context.fillStyle = "#646464";
  context.fill();
  
}

function drawRightTriangle(context){
  // Draw a path
  context.beginPath();
  context.moveTo(0, 0);        // Top Corner
  context.lineTo(8,0); // Bottom Right
  context.lineTo(0, 8);         // Bottom Left
  context.closePath();

  // Fill the path
  context.fillStyle = "#646464";//#d8cc44";
  context.fill();
  
}

